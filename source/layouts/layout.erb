<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <meta name="viewport" content="width=device-width,initial-scale=1.0,shrink-to-fit=no, user-scalable=no" />
    <title><%= current_page.data.title %></title>
    <link rel="shortcut icon" href="favicon.ico">
    <%= partial "_partial/social" %>
    <%= stylesheet_link_tag "normalize", "style", "slick" %>
    <link href='https://fonts.googleapis.com/css?family=Crimson+Text:700' rel='stylesheet' type='text/css'>
  </head>

  <body class="<%= page_classes %>">
    <%= yield %>
  </body>
  <script   src="https://code.jquery.com/jquery-3.1.0.min.js" integrity="sha256-cCueBR6CsyA4/9szpPfrX3s49M9vUU5BgtiJj06wt/s=" crossorigin="anonymous"></script>
  <%= javascript_include_tag  "all" %>
  <div id="fb-root"></div><script async src="//platform.twitter.com/widgets.js" id="twitter-wjs"></script>
  <script src="//connect.facebook.net/ja_JP/sdk.js#xfbml=1&version=v2.0" id="facebook-jssdk" async></script>
</html>
